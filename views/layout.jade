!!!
html(xmlns="http://www.w3.org/1999/xhtml", kaka='kaki', a:b='muma', xmlns:fb="http://www.facebook.com/2008/fbml", xml:lang="en", lang="en")
  head
    - var title = 'קולורבים'
    - if (typeof vote != 'undefined' && vote)
      - title += ' - ' + vote.data.title;
    title= title
    link(rel="stylesheet", href="/sass/theme.css")
    link(rel="stylesheet", href="/fb1.css")
    link(rel="stylesheet", href="/fb2.css")
    link(href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css", rel="stylesheet", type="text/css")
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js")
    script(src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.7/jquery-ui.min.js")
    script(src="/sprklines.js")
  body
    #fb-root
    script
      |window.fbAsyncInit = function() {      
      |FB.init({
      |appId  : '140153199345253',
      |status : true, // check login status
      |cookie : true, // enable cookies to allow the server to access the session
      |xfbml  : true  // parse XFBML
      |});
      |};
      |(function() {
      |var e = document.createElement('script');
      |e.async = true;
      |e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
      |document.getElementById('fb-root').appendChild(e);
      |}());
    script(src='/fbconnect.js')
    script(src='/clientvotes.js')
    #content!= body